[theme]

[theme.options]
is_display = true
display_time = "waveshare144lcd"
resolution = [128, 128]
hi-res = true
bg = true
bg_image = "ui-bg-w-s.png"
#Bold, BoldSmall, Medium, Huge, BoldBig, Small
font_sizes = [10, 8, 10, 25, 25, 9]
